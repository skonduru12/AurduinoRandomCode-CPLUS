int sensor_pin = A5;
int led = 2;


void setup() {
 
  Serial.begin(9600);
  pinMode(sensor_pin, INPUT);
  pinMode(led, OUTPUT);


}


void loop() {


int sensordata = analogRead(A5);
Serial.println(sensordata);
delay(200);
if(sensordata < 500) {
  digitalWrite(led, LOW);
}
else   {
  digitalWrite(led, HIGH);
}
delay(200);
}
